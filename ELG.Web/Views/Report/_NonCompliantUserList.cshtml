<div class="modal fade" id="learnerListForNonCompliantModal" tabindex="-1" role="dialog" aria-labelledby="learnerListForNonCompliantModal" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"></h5>
                <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="card shadow mb-4">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table" id="learnerListFor_NonCompliant" width="100%" cellspacing="0">
                                <thead>
                                    <tr>
                                        @*<th>@ELG.Web.Helper.SessionHelper.CompanySettings.strEmployeeNumberDescription</th>*@
                                        <th>User</th>
                                        <th>Email</th>
                                        <th>@ELG.Web.Helper.SessionHelper.CompanySettings.strLocationDescription</th>
                                        <th>@ELG.Web.Helper.SessionHelper.CompanySettings.strDepartmentDescription</th>
                                        <th>Status</th>
                                        <th>Completed On</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-secondary" type="button" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>



